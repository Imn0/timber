@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# Ensure the main target exists
if(TARGET @PROJECT_NAME@::shared AND NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
    add_library(@PROJECT_NAME@::@PROJECT_NAME@ ALIAS @PROJECT_NAME@::shared)
elseif(TARGET @PROJECT_NAME@::static AND NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
    add_library(@PROJECT_NAME@::@PROJECT_NAME@ ALIAS @PROJECT_NAME@::static)
endif()

check_required_components(@PROJECT_NAME@)
