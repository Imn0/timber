@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# Create namespaced default alias
if(TARGET @PROJECT_NAME@::shared)
    if(NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
        add_library(@PROJECT_NAME@::@PROJECT_NAME@ ALIAS @PROJECT_NAME@::shared)
    endif()
    # Also create non-namespaced alias
    if(NOT TARGET @PROJECT_NAME@)
        add_library(@PROJECT_NAME@ ALIAS @PROJECT_NAME@::shared)
    endif()
elseif(TARGET @PROJECT_NAME@::static)
    if(NOT TARGET @PROJECT_NAME@::@PROJECT_NAME@)
        add_library(@PROJECT_NAME@::@PROJECT_NAME@ ALIAS @PROJECT_NAME@::static)
    endif()
    # Also create non-namespaced alias
    if(NOT TARGET @PROJECT_NAME@)
        add_library(@PROJECT_NAME@ ALIAS @PROJECT_NAME@::static)
    endif()
endif()